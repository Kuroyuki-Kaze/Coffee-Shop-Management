    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "mainClass": "com.client.App",
            "projectName": "demo"
        },
        {
            "type": "java",
            "name": "Launch Crypter",
            "request": "launch",
            "mainClass": "com.helper.Crypter",
            "projectName": "demo"
        },
        {
            "type": "java",
            "name": "Launch Main",
            "request": "launch",
            "mainClass": "com.example.Main",
            "projectName": "demo"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "mainClass": "com.example.App",
            "projectName": "demo"
        },
        {
            "type": "java",
            "name": "Launch Current JavaFX file",
            "request": "launch",
            "mainClass": "${file}",
            "vmArgs": "--module-path C:/jfx/javafx-sdk-17.0.2/lib --add-modules javafx.controls,javafx.fxml,javafx.graphics,javafx.media,javafx.swing,javafx.web,javafx.swt"
        }
    ]
}